public class Main {
  public static void main(String[] args) {
    int i = 1; // 0 yerine 1 çünkü 1–10 arasında istiyoruz

    while (i <= 10) {
      if (i == 4) {
        i++;
        continue; // 4’ü atla
      }

      if (i == 7) {
        break; // 7’de döngüyü sonlandır
      }

      System.out.println(i); // sadece 4 ve 7 dışında yazdır
      i++;
    }
  }
}
